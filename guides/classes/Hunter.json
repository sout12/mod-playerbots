{
  "class": "Hunter",
  "version": "3.3.5a",
  "roles": [
    "ranged_dps",
    "kiter",
    "flag_hunter",
    "objective_support"
  ],
  "game_state": {
    "early_game": {
      "focus": "establish presence, contest objectives",
      "priority": ["maintain_vision", "apply_pressure", "maintain_positioning"]
    },
    "mid_game": {
      "focus": "control key objectives, engage in team fights",
      "priority": ["flag_carrier_hunting", "objective_control", "team_fight_support"]
    },
    "late_game": {
      "focus": "secure win condition, defend lead",
      "priority": ["hunt_flag_carrier", "defend_objectives", "close_out_game"]
    }
  },
  "general_priority_list": [
    "use_kiting_and_slows_on_flag_carriers",
    "pet_control_to_interrupt_and_peel",
    "deterrence_for_big_burst_mitigation",
    "maintain_distance_control",
    "trap_placement_for_area_control",
    "focus_fire_on_high_priority_targets",
    "maintain_vision_and_tracking"
  ],
  "spell_rules": {
    "crowd_control": [
      {
        "name": "Concussive Shot",
        "when": "Slow fleeing or prevent escape",
        "priority": "high",
        "conditions": [
          "target_is_fleeing == true",
          "or target_is_flag_carrier_moving_away == true",
          "or target_is_trying_to_escape == true"
        ],
        "target": "enemy",
        "context_weights": {
          "flag_carrier": 2.0,
          "fleeing_target": 1.5,
          "escape_attempt": 1.2
        }
      },
      {
        "name": "Scatter Shot",
        "when": "Peel melee or interrupt casters",
        "priority": "very_high",
        "conditions": [
          "target_is_casting == true",
          "or melee_aggro_on_pet == true",
          "or ally_is_under_melee_attack == true"
        ],
        "target": "enemy",
        "context_weights": {
          "interrupt_caster": 2.0,
          "peel_melee": 1.8,
          "protect_ally": 1.5
        }
      },
      {
        "name": "Freezing Trap",
        "when": "Control area or CC high priority targets",
        "priority": "high",
        "conditions": [
          "high_priority_target_in_area == true",
          "or area_control_needed == true",
          "or need_to_escape_dangerous_area == true"
        ],
        "target": "area",
        "context_weights": {
          "high_priority_target": 2.0,
          "area_control": 1.5,
          "escape_necessity": 1.2
        }
      },
      {
        "name": "Wyvern Sting",
        "when": "Silence and sleep target",
        "priority": "very_high",
        "conditions": [
          "target_is_healer == true",
          "or target_is_flag_carrier == true",
          "or target_is_high_priority_caster == true"
        ],
        "target": "enemy",
        "context_weights": {
          "healer": 2.0,
          "flag_carrier": 1.8,
          "high_priority_caster": 1.5
        }
      },
      {
        "name": "Intimidation",
        "when": "Stun target with pet",
        "priority": "high",
        "conditions": [
          "pet_active == true",
          "or target_is_casting_emergency_spell == true",
          "or interrupt_opportunity_available == true"
        ],
        "target": "enemy",
        "context_weights": {
          "pet_available": 2.0,
          "emergency_interrupt": 1.8,
          "interrupt_opportunity": 1.5
        }
      }
    ],
    "defensive": [
      {
        "name": "Feign Death",
        "when": "Drop threat and reset combat",
        "priority": "high",
        "conditions": [
          "focus_target_is_badly_overgeared == true",
          "or overwhelmed == true",
          "or need_to_reset_combat == true"
        ],
        "target": "self",
        "context_weights": {
          "overgeared_target": 2.0,
          "overwhelmed": 1.8,
          "combat_reset": 1.5
        }
      },
      {
        "name": "Deterrence",
        "when": "Mitigate burst damage",
        "priority": "very_high",
        "conditions": [
          "incoming_burst_damage >= 2000",
          "or self_hp_percent <= 40",
          "or multiple_attackers_targeting == true"
        ],
        "target": "self",
        "context_weights": {
          "lethal_damage": 2.0,
          "multiple_attackers": 1.8,
          "high_damage": 1.5
        }
      },
      {
        "name": "Disengage",
        "when": "Create distance and escape",
        "priority": "high",
        "conditions": [
          "enemy_too_close == true",
          "or need_to_escape_melee == true",
          "or positioning_adjustment_needed == true"
        ],
        "target": "self",
        "context_weights": {
          "escape_necessity": 2.0,
          "positioning_adjustment": 1.5,
          "melee_escape": 1.2
        }
      },
      {
        "name": "Aspect of the Pack",
        "when": "Increase movement speed for flag running",
        "priority": "high",
        "conditions": [
          "self_is_flag_carrier == true",
          "or need_to_escape == true",
          "or positioning_adjustment_needed == true"
        ],
        "target": "self",
        "context_weights": {
          "flag_carrier": 2.0,
          "escape_necessity": 1.8,
          "positioning": 1.5
        }
      }
    ],
    "interrupts": [
      {
        "name": "Silencing Shot",
        "when": "Interrupt casting at range",
        "priority": "very_high",
        "conditions": [
          "target_is_casting == true",
          "or target_is_healer_casting == true",
          "or target_is_casting_high_priority_spell == true"
        ],
        "target": "enemy",
        "context_weights": {
          "healer_casting": 2.0,
          "high_priority_spell": 1.8,
          "casting_interrupt": 1.5
        }
      }
    ],
    "utility": [
      {
        "name": "Tranquilizing Shot",
        "when": "Remove enrage or buff from boss type NPCs",
        "priority": "low",
        "conditions": [
          "npc_has_enrage == true",
          "or enemy_has_magic_buff == true",
          "or buff_is_combat_relevant == true"
        ],
        "target": "enemy",
        "context_weights": {
          "npc_enrage": 2.0,
          "magic_buff": 1.5,
          "combat_relevant": 1.2
        }
      },
      {
        "name": "Hunter's Mark",
        "when": "Maintain damage debuff",
        "priority": "high",
        "conditions": [
          "target_does_not_have_hunters_mark == true",
          "or target_is_high_priority == true",
          "or focus_fire_target == true"
        ],
        "target": "enemy",
        "context_weights": {
          "high_priority_target": 2.0,
          "focus_fire": 1.8,
          "maintain_debuff": 1.5
        }
      }
    ],
    "utility": [
      {
        "name": "Tranquilizing Shot",
        "when": "Remove enrage or buff from boss type NPCs",
        "priority": "low",
        "conditions": [
          "npc_has_enrage == true",
          "or enemy_has_magic_buff == true",
          "or buff_is_combat_relevant == true"
        ],
        "target": "enemy",
        "context_weights": {
          "npc_enrage": 2.0,
          "magic_buff": 1.5,
          "combat_relevant": 1.2
        }
      },
      {
        "name": "Hunter's Mark",
        "when": "Maintain damage debuff",
        "priority": "high",
        "conditions": [
          "target_does_not_have_hunters_mark == true",
          "or target_is_high_priority == true",
          "or focus_fire_target == true"
        ],
        "target": "enemy",
        "context_weights": {
          "high_priority_target": 2.0,
          "focus_fire": 1.8,
          "maintain_debuff": 1.5
        }
      },
      {
        "name": "Misdirection",
        "when": "Redirect aggro to pet or ally",
        "priority": "high",
        "conditions": [
          "ally_is_tank_under_attack == true",
          "or need_to_redirect_aggro == true",
          "or pet_aggro_opportunity == true"
        ],
        "target": "ally",
        "context_weights": {
          "tank_protection": 2.0,
          "aggro_redirection": 1.8,
          "pet_utility": 1.5
        }
      },
      {
        "name": "Feign Death",
        "when": "Drop all aggro and reset",
        "priority": "high",
        "conditions": [
          "overwhelmed_by_enemies == true",
          "or need_to_reset_combat == true",
          "or escape_necessity == true"
        ],
        "target": "self",
        "context_weights": {
          "overwhelmed": 2.0,
          "combat_reset": 1.8,
          "escape_necessity": 1.5
        }
      }
    ],
    "damage": [
      {
        "name": "Aimed Shot",
        "when": "High damage shot with focus",
        "priority": "high",
        "conditions": [
          "focus_available_for_aimed_shot == true",
          "or target_is_low_hp == true",
          "or need_to_burst_target == true"
        ],
        "target": "enemy",
        "context_weights": {
          "burst_opportunity": 2.0,
          "low_hp_target": 1.8,
          "focus_available": 1.5
        }
      },
      {
        "name": "Chimera Shot",
        "when": "Apply damage and heal",
        "priority": "medium",
        "conditions": [
          "self_hp_percent <= 80",
          "or target_has_serious_wound == true",
          "or sustained_combat_engagement == true"
        ],
        "target": "enemy",
        "context_weights": {
          "self_heal_needed": 2.0,
          "sustained_combat": 1.5,
          "damage_opportunity": 1.2
        }
      },
      {
        "name": "Explosive Shot",
        "when": "Damage over time and DoT",
        "priority": "medium",
        "conditions": [
          "target_does_not_have_explosive_shot == true",
          "or explosive_shot_expiring_soon == true",
          "or sustained_damage_over_time_needed == true"
        ],
        "target": "enemy",
        "context_weights": {
          "no_explosive_shot": 2.0,
          "expiring_soon": 1.8,
          "sustained_damage": 1.5
        }
      },
      {
        "name": "Serpent Sting",
        "when": "Damage over time",
        "priority": "high",
        "conditions": [
          "target_does_not_have_serpent_sting == true",
          "or serpent_sting_expiring_soon == true",
          "or sustained_damage_over_time_needed == true"
        ],
        "target": "enemy",
        "context_weights": {
          "no_serpent_sting": 2.0,
          "expiring_soon": 1.8,
          "sustained_damage": 1.5
        }
      },
      {
        "name": "Arcane Shot",
        "when": "Primary damage shot",
        "priority": "high",
        "conditions": [
          "target_vulnerable == true",
          "or damage_opportunity_available == true",
          "or sustained_damage_needed == true"
        ],
        "target": "enemy",
        "context_weights": {
          "vulnerable_target": 2.0,
          "damage_opportunity": 1.8,
          "sustained_damage": 1.5
        }
      }
    ]
  },
  "pet_logic": {
    "use_pet_to_kite": true,
    "pet_focus_fire_on_flag_carrier": true,
    "pet_behavior_by_role": {
      "kiter": ["pet_tank_melee", "pet_peel_attackers", "maintain_distance"],
      "damage": ["pet_focus_fire", "pet_attack_high_priority", "pet_maintain_aggro"],
      "support": ["pet_control_area", "pet_interrupt_casters", "pet_peel_ally"]
    },
    "pet_commands": [
      "if_enemy_approaching -> pet_attack_target",
      "if_need_to_escape -> pet_taunt_attackers",
      "if_ally_under_attack -> pet_peel_target"
    ]
  },
