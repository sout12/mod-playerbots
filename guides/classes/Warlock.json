        "target": "enemy",
        "context_weights": {
          "no_haunt": 2.0,
          "expiring_soon": 1.8,
          "mana_drain": 1.5
        }
      }
    ],
    "defensive": [
      {
        "name": "Unending Breath",
        "when": "Maintain underwater breathing",
        "priority": "low",
        "conditions": [
