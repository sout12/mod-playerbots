  "spell_rules": {
    "crowd_control": [
      {
        "name": "Fear",
        "when": "Peel melee from healers or grouped targets",
        "priority": "high",
        "conditions": [
          "melee_engaged_with_friendly_healer >= 1",
          "or multiple_enemies_approaching == true",
          "or need_to_control_area == true"
        ],
        "target": "enemy_group",
        "context_weights": {
          "healer_protection": 2.0,
          "multiple_targets": 1.8,
          "area_control": 1.5,
          "peel_necessity": 1.2
        }
      },
      {
        "name": "Howl of Terror",
        "when": "AoE fear for escape or control",
        "priority": "high",
        "conditions": [
          "multiple_enemies_in_melee_range == true",
          "or need_to_escape_dangerous_situation == true",
          "or area_control_opportunity == true"
        ],
        "target": "area",
        "context_weights": {
          "escape_necessity": 2.0,
          "multiple_enemies": 1.8,
          "area_control": 1.5,
          "aoe_fear": 1.2
        }
      },
      {
        "name": "Banish",
        "when": "Remove target from combat temporarily",
        "priority": "medium",
        "conditions": [
          "target_is_low_level_or_immune_to_fear == true",
          "or tactical_advantage_opportunity == true",
          "or need_to_isolate_enemy == true"
        ],
        "target": "enemy",
        "context_weights": {
          "tactical_advantage": 2.0,
          "isolation_opportunity": 1.8,
          "immune_to_fear": 1.5
        }
      }
    ],
    "interrupts": [
      {
        "name": "Spell Lock",
        "when": "Pet interrupt of caster or healer",
        "priority": "very_high",
        "conditions": [
          "target_is_casting == true",
          "or target_is_healer_casting_emergency_heal == true",
          "or target_is_casting_high_priority_spell == true"
        ],
        "target": "enemy",
        "context_weights": {
          "healer_casting": 2.0,
          "emergency_heal": 1.8,
          "high_priority_spell": 1.5,
          "casting_interrupt": 1.2
        }
      }
    ],
    "damage": [
      {
        "name": "Death Coil",
        "when": "Instant damage or heal",
        "priority": "medium",
        "conditions": [
          "self_hp_low_or_need_nuke == true",
          "or target_is_low_hp == true",
          "or need_instant_damage_or_heal == true"
        ],
        "target": "enemy_or_self",
        "context_weights": {
          "self_heal_needed": 2.0,
          "low_hp_target": 1.8,
          "instant_damage": 1.5,
          "utility_spell": 1.2
        }
      },
      {
        "name": "Shadow Bolt",
        "when": "Primary damage spell",
        "priority": "high",
        "conditions": [
          "target_is_vulnerable == true",
          "or clear_cast_available == true",
          "or need_to_apply_damage == true"
        ],
        "target": "enemy",
        "context_weights": {
          "vulnerable_target": 2.0,
          "clear_cast": 1.8,
          "primary_damage": 1.5,
          "sustained_damage": 1.2
        }
      },
      {
        "name": "Incinerate",
        "when": "Execute low health targets",
        "priority": "high",
        "conditions": [
          "target_hp_percent <= 20",
          "or need_to_execute_low_hp == true",
          "or burning_embers_available == true"
        ],
        "target": "enemy",
        "context_weights": {
          "execute_opportunity": 2.0,
          "low_hp_target": 1.8,
          "burning_embers": 1.5,
          "execute_phase": 1.2
        }
      },
      {
        "name": "Conflagrate",
        "when": "Crit dot damage",
        "priority": "high",
        "conditions": [
          "target_has_immolate == true",
          "or need_to_crit_dot_damage == true",
          "or burning_embers_available == true"
        ],
        "target": "enemy",
        "context_weights": {
          "immolate_present": 2.0,
          "crit_damage": 1.8,
          "burning_embers": 1.5,
          "dot_crit": 1.2
        }
      },
      {
        "name": "Soul Fire",
        "when": "High damage with DoT",
        "priority": "high",
        "conditions": [
          "target_does_not_have_immolate == true",
          "or need_high_damage_burst == true",
          "or burning_embers_available == true"
        ],
        "target": "enemy",
        "context_weights": {
          "no_immolate": 2.0,
          "high_damage_burst": 1.8,
          "burning_embers": 1.5
        }
      }
    ],
    "damage_over_time": [
      {
        "name": "Corruption",
        "when": "Maintain primary DoT",
        "priority": "high",
        "conditions": [
          "target_does_not_have_corruption == true",
          "or corruption_expiring_soon == true",
          "or sustained_damage_over_time_needed == true"
        ],
        "target": "enemy",
        "context_weights": {
          "no_corruption": 2.0,
          "expiring_soon": 1.8,
          "sustained_damage": 1.5,
          "primary_dot": 1.2
        }
      },
      {
        "name": "Immolate",
        "when": "Apply fire DoT",
        "priority": "high",
        "conditions": [
          "target_does_not_have_immolate == true",
          "or immolate_expiring_soon == true",
          "or need_to_apply_fire_damage_over_time == true"
        ],
        "target": "enemy",
        "context_weights": {
          "no_immolate": 2.0,
          "expiring_soon": 1.8,
          "fire_damage": 1.5,
          "secondary_dot": 1.2
        }
      },
      {
        "name": "Curse of Agony",
        "when": "Sustained DoT damage",
        "priority": "medium",
        "conditions": [
          "target_does_not_have_curse == true",
          "or curse_expiring_soon == true",
          "or sustained_damage_over_time_needed == true"
        ],
        "target": "enemy",
        "context_weights": {
          "no_curse": 2.0,
          "expiring_soon": 1.8,
          "sustained_damage": 1.5,
          "sustained_dot": 1.2
        }
      },
      {
        "name": "Unstable Affliction",
        "when": "High damage DoT",
        "priority": "high",
        "conditions": [
          "target_does_not_have_ua == true",
          "or ua_expiring_soon == true",
          "or high_damage_dot_needed == true"
        ],
        "target": "enemy",
        "context_weights": {
          "no_ua": 2.0,
          "expiring_soon": 1.8,
          "high_damage": 1.5,
          "high_dot_damage": 1.2
        }
      },
      {
        "name": "Haunt",
        "when": "High damage DoT with mana drain",
        "priority": "high",
        "conditions": [
          "target_does_not_have_haunt == true",
          "or haunt_expiring_soon == true",
          "or mana_drain_opportunity == true"
