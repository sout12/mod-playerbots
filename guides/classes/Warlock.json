        "context_weights": {
          "key_player": 2.0,
          "combat_res": 1.8,
          "revival_opportunity": 1.5
        }
      },
      {
        "name": "Teleport (any)",
        "when": "Escape while flag carrier (causes flag drop)",
        "priority": "low",
        "conditions": [
          "self_is_flag_carrier == true and danger_imminent == true"
        ],
        "target": "self",
        "context_weights": {
          "flag_carrier_in_danger": 2.0,
          "escape_necessity": 1.8,
          "avoidable_flag_drop": 1.5
        },
        "flag_mechanics": {
          "drops_flag_when_used": true,
          "avoid_when_flag_carrier": true,
          "alternative_escapes": [
            "fear_kiting",
            "pet_escape",
            "shadowfury_stun"
          ]
        }
      },
      {
        "name": "Demonic Circle: Summon",
        "when": "Place portal in Line of Sight/Safe spot",
        "priority": "very_high",
        "conditions": [
          "portal_active == false",
          "safe_location == true",
          "near_pillar_or_los == true"
        ],
        "target": "ground_position"
      }
