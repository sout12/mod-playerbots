        "hunt_mid_attackers",
        "maintain_flag_access_control"
      ],
      "role_assignments": {
        "mid_controller": ["control_mid", "deny_flag_access", "hunt_attackers"],
        "flag_protector": ["escort_carrier", "interrupt_escorts", "maintain_vision"],
        "base_defender": ["defend_bases", "kill_cappers", "maintain_vision"]
      }
    },
    "SOTA": {
