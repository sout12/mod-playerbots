        "offensive_positions": ["approach_angles", "flank_routes", "control_points"]
      }
    }
  },
  "resource_management": {
    "energy_efficiency": {
      "spending_strategy": ["save_for_emergency", "spend_on_opportunity", "maintain_efficiency"],
      "priority_targets": ["burst_damage", "sustained_damage", "utility_benefits"]
    },
    "combo_point_utilization": {
      "rotation_priority": ["execute_opportunities", "damage_over_time", "utility_uses"],
      "emergency_reserves": ["eviscerate_for_heal", "rupture_for_sustained", "garrote_for_silence"]
