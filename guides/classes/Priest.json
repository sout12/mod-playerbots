  "spell_rules": {
    "protective_shields": [
      {
        "name": "Power Word: Shield",
        "when": "Preemptive protection for healers/tanks",
        "priority": "very_high",
        "conditions": [
          "ally_is_about_to_be_targeted == true",
          "or ally_is_flag_carrier == true",
          "or ally_is_healer_and_vulnerable == true"
        ],
        "target": "ally",
        "context_weights": {
          "flag_carrier": 2.0,
          "healer": 1.8,
          "vulnerable_ally": 1.5,
          "tank": 1.2
        }
      }
    ],
    "interrupts": [
      {
        "name": "Silence",
        "when": "Silence healers or casters",
        "priority": "very_high",
        "conditions": [
