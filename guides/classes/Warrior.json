          "or target_is_fleeing == true",
          "or need_to_reach_target_quickly == true"
        ],
        "target": "enemy",
        "context_weights": {
          "fleeing_target": 2.0,
          "gap_closing": 1.8,
          "reach_target": 1.5,
          "utility_gap_close": 1.2
