        "target": "area",
        "context_weights": {
          "multiple_targets": 1.8,
          "sustained_combat": 1.5,
          "approach_control": 1.2
        }
      },
      {
        "name": "Concussion Blow",
        "when": "Interrupt casting with stun",
        "priority": "very_high",
        "conditions": [
          "target_is_casting == true",
          "or target_is_healer_casting == true",
